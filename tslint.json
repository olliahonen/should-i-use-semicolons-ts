{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      false
    ],
    "no-console": [
      false
    ],
    "quotemark": [
      true,
      "single"
    ],
    "arrow-parens": false,
    "no-unexpected-multiline": true
  },
  "rulesDirectory": []
}
